{
  "name": "urls-decoder",
  "version": "1.1.4",
  "private": true,
  "dependencies": {
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.0",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-react": "react-scripts build",
    "build-fix": "node scripts/replace-inline-script.js",
    "build": "npm run build-react && npm run build-fix",
    "zip-build": "node scripts/zip-build.js",
    "update-ver": "node scripts/update-version.js",
    "prod": "npm run build && npm run zip-build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css}": "prettier --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "adm-zip": "^0.4.16",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1"
  }
}
