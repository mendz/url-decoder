{
  "name": "urls-decoder",
  "version": "1.1.6",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^4.0.3",
    "react-toastify": "^7.0.3"
  },
  "scripts": {
    "start": "craco start",
    "build-react": "craco build",
    "build-fix": "node scripts/replace-inline-script.js",
    "build": "npm run build-react && npm run build-fix",
    "zip-build": "node scripts/zip-build.js",
    "update-ver": "node scripts/update-version.js",
    "prod": "npm run build && npm run zip-build",
    "test": "craco test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "@types/jest": "^26.0.20",
    "adm-zip": "^0.5.4",
    "autoprefixer": "^9.8.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-extra": "^9.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^7.0.35",
    "prettier": "^2.2.1",
    "react-test-renderer": "^16.13.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,html}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
